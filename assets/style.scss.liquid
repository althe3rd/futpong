.row.no-gutter {
	margin: 0px;
	justify-content: center;
	
	.child {
		flex: 1 0 auto;
		-ms-flex: 1 0 auto;
		-webkit-flex: 1 0 auto;
		
		
		
		text-align: center;
		height: auto;
		position: relative;
		
		a:hover {
			.cardShade {
				position: absolute;
				top: 0px;
				left: 0px;
				right: 0px;
				bottom: 0px;
				z-index:2;
				background: rgba(0,0,0,0.0);
			}
			
			.expandIcon {
				transform: translate(-50%,-50%) rotate(90deg);
				-webkit-transform: translate(-50%,-50%) rotate(90deg);
				transform-origin: center center;
				-webkit-transform-origin: center center;
			}
		}
		
		a {
			display: block;
			position: absolute;
			top: 0px;
			left: 0px;
			right: 0px;
			bottom: 0px;
			z-index:1;
			
			.cardShade {
				position: absolute;
				top: 0px;
				left: 0px;
				right: 0px;
				bottom: 0px;
				z-index:2;
				transition: 0.4s;
				background: rgba(0,0,0,0.2);
			}
			
			.sharpenTitle {
				position: absolute;
				top: 50%;
				left: 50%;
				z-index:3;
				color: #fff;
				font-size: 24px;
				letter-spacing: 1.5px;
				text-transform: uppercase;
				transform: translate(-50%,-50%);
				-webkit-transform: translate(-50%,-50%);
				
			}
			
			.expandIcon {
				position: absolute;
				top: 65%;
				left: 50%;
				z-index:3;
				transform: translate(-50%,-50%) rotate(0deg);
				-webkit-transform: translate(-50%,-50%) rotate(0deg);
				width: 30px;
				height: 30px;
				transform-origin: center, center;
				-webkit-transform-origin: center, center;
				transition: 0.4s;
				
				svg {
					width: 100%;
					height: 100%;
					
					path, polygon {
						fill: rgba(255,255,255,0.9);
					}
				}
			}
		}
		
	}
	
	.child:before {
		content:'';
		float:left;
		padding-top:100%;
	}
}

